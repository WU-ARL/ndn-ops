	iface pkutoucla inet static
	address 10.0.3.14
	netmask 255.255.255.252
	pre-up iptunnel add pkutoucla  mode gre local 162.105.146.26 remote 131.179.196.46 ttl 255
	pointopoint 10.0.3.13
	up ifconfig pkutoucla multicast

